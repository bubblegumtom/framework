{
    "name": "laraish/framework",
    "description": "The Laraish Framework for WordPress.",
    "keywords": [
        "framework",
        "laravel",
        "wordpress"
    ],
    "license": "MIT",
    "homepage": "http://laravel.com",
    "support": {
        "issues": "https://github.com/laravel/framework/issues",
        "source": "https://github.com/laravel/framework"
    },
    "authors": [
        {
            "name": "yaquawa",
            "email": "yaquawa@gmail.com"
        }
    ],
    "repositories": {
        "contracts": {
            "type": "vcs",
            "url": "https://github.com/bubblegumtom/contracts"
        },
        "support": {
            "type": "vcs",
            "url": "https://github.com/bubblegumtom/support"
        },
        "pagination": {
            "type": "vcs",
            "url": "https://github.com/bubblegumtom/pagination"
        },
        "options": {
            "type": "vcs",
            "url": "https://github.com/bubblegumtom/options"
        },
    },
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0",
        "laraish/contracts": "dev-master",
        "laraish/support": "dev-master",
        "laraish/pagination": "dev-master",
        "laraish/options": "dev-master"
    },
    "autoload": {
        "files": [
            "Foundation/helpers.php"
        ],
        "psr-4": {
            "Laraish\\": "./"
        }
    },
    "scripts": {
        "fix-cs": ["prettier **/*.php --write"]
    }
}
